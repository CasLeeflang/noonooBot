<robot name="noonoo">
  <link name="base_link" />
  <link name="wheel_left" />
  <link name="wheel_right" />
  <link name="laser" />
  <link name="base_footprint" />

  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0 0 0.035977" rpy="0 0 0"/>
  </joint>

  <joint name="wheel_leftjoint" type="continuous">
    <parent link="base_link"/>
    <child link ="wheel_left"/>
    <origin xyz="0 -0.12851 0.0" rpy="0 0 0"/>
  </joint>

  <joint name="wheel_rightjoint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_right"/>
    <origin xyz="0 0.12949 0.0" rpy="0 0 0"/>
  </joint>

  <joint name="laser_joint" type="fixed">
    <parent link="base_link"/>
    <child link="laser"/>
    <origin xyz="0.0 0.0 0.069556" rpy="0 0 0"/>
  </joint>
</robot>
